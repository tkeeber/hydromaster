package com.sprint22.apps.android.hydration.utils;

public class SharedPerferencesEntry<V> {

	private final String key;
	private final V value;

	public SharedPerferencesEntry(String key, V v) {
		this.key = key;
		this.value = v;
	}

	public String getKey() {
		return key;
	}

	public V getValue() {
		return value;
	}

	public String toString() {
		return "(" + key + ", " + value + ")";
	}

}
